    <!-- Cadastro de produtos -->
    <section class="">
      <div class="container">

        <!-- Formuário de cadastro -->
        <div class="row justify-content-center">
          <div class="col-xl-8 col-lg-9">
            <h1 class="text-center">Cadastro de produto</h1>
            <c:url value="/produto/register" var="registerUrl" />
            <form action="${registerUrl}" method="post">
              <c:if test="${produto.idProduto ne null}">
                <div class="form-group">
                  <label for="idProduto">ID:</label>
                  <input type="text" class="form-control" id="idProduto" name="idProduto" value="${produto.idProduto}" placeholder="0000" readonly>
                </div>
              </c:if>
              <div class="form-group">
                <label for="NomeProduto">Nome:</label>
                <input type="text" class="form-control" id="NomeProduto" name="NomeProduto" value="${produto.nomeProduto}" required="true" placeholder="Nome do produto">
              </div>
              <div class="form-group">
                <label for="Ano">Ano:</label>
                <input type="text" class="form-control" id="Ano" name="Ano" value="${produto.ano}" required="true" placeholder="Ano do veículo">
              </div>
              <div class="form-group">
                <label for="Modelo">Modelo:</label>
                <input type="text" class="form-control" id="Modelo" name="Modelo" value="${produto.modelo}" required="true" placeholder="Modelo do veículo">
              </div>
              <div class="form-group">
                <label for="Marca">Marca:</label>
                <input type="text" class="form-control" id="Marca" name="Marca" value="${produto.marca}" required="true" placeholder="Marca do veículo">
              </div>
              <div class="form-group">
                <label for="input-group-example">Placa:</label>
                <input type="text" class="form-control" id="input-group-example" aria-describedby="input-group-example" placeholder="Placa do veículo">
              </div>
              <div class="form-group">
                <label for="input-group-example">Valor da diária:</label>
                <input type="text" class="form-control" id="input-group-example" aria-describedby="input-group-example" placeholder="Valor da diária do veículo">
              </div>
              <c:if test="${produto.idProduto ne null}">
                <div class="form-group mt-5">
                  <button class="btn-block btn btn-primary-2" value="Update" type="submit">Editar</button>
                </div>
              </c:if>
              <c:if test="${produto.idProduto eq null}">
                <div class="form-group mt-5">
                  <button class="btn-block btn btn-primary" value="Save" type="submit">Salvar</button>
                </div>
              </c:if>
            </form>
          </div>
        </div>

      </div>
    </section>

    <!-- Lista de produtos -->
